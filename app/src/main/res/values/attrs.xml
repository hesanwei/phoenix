<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MineItemView">
        <attr name="mivImage" format="reference" />
        <attr name="mivName" format="string" />
        <attr name="mivValue" format="reference" />
        <attr name="mivShowDivider" format="boolean" />
    </declare-styleable>

    <!--    倒计时TextView-->
    <declare-styleable name="CountdownTextView">
        <attr name="ctvDefaultCount" format="integer"/>
        <attr name="ctvStopText" format="string"/>
        <attr name="ctvStopTextColor" format="color"/>
        <attr name="ctvCountTextColor" format="color"/>
    </declare-styleable>
</resources>